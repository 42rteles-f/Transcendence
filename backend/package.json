{
	"main": "./src/index.ts",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"start": "node ./dest/index.js",
		"dev": "nodemon --watch './src/**/*.ts' --exec 'ts-node' ./src/index.ts",
		"clear": "rm -rf ./dest",
		"prune": "npm run clear && rm -rf ./node_modules ./package-lock.json",
		"migrate": "node ./migrations/migrate.js"
	},
	"dependencies": {
		"@fastify/cors": "^11.0.1",
		"bcrypt": "^6.0.0",
		"fastify": "^5.3.1",
		"fastify-plugin": "^5.0.1",
		"jsonwebtoken": "^9.0.2",
		"knex": "^3.1.0",
		"sqlite3": "^5.1.7"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/ms": "^2.1.0",
		"@types/node": "^22.15.23",
		"@types/pg": "^8.15.2",
		"nodemon": "^3.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	}
}
